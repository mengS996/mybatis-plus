<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ms.mybatisplus.mapper.UserMapper">
    <!--设置sql片段-->
    <sql id="Base_Column_List">
        id,name,age,email
    </sql>

    <select id="selectAllByName" resultType="com.ms.mybatisplus.entity.User">
        select <include refid="Base_Column_List"></include>
        from user
        where name= #{name}
    </select>

    <select id="selectPageByPage" resultType="com.ms.mybatisplus.entity.User">
        select <include refid="Base_Column_List"></include>
        from user
        where age > #{age}
    </select>
</mapper>
